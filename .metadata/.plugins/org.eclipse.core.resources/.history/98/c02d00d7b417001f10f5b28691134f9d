package models;
import java.util.Date;

public class Tarea extends Categoria {
	
	public int id;
	public String texto;
	public boolean completada;
	public Date fecha;
	
	public Tarea(int id, String nombre, int id2, String texto, boolean completada, Date fecha) {
		super(id, nombre);
		id = id2;
		this.texto = texto;
		this.completada = completada;
		this.fecha = fecha;
	}

	
	@Override
	public String toString() {
		return "Tarea :  id=" + id +"\n"+
				"texto:  texto=" + texto +"\n"+
				"completada: completada=" + completada + " \n"+
				"fecha: fecha=" + fecha + "\n"; 
				
	}
	

	
	
}
